<?xml-stylesheet type="text/xsl" href="ospf-mt-routing.xsl"?>
<?oxygen RNGSchema="ospf-mt-routing.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="ospf-mt-routing">
  <title>Open Shortest Path First (OSPF) Multi-Topology Routing</title>
  <created>2007-06-04</created>
  <updated>2013-05-28</updated>
  <registry id="ospf-mt-routing-2">
    <title>Multi-Topology ID Values</title>
    <xref type="rfc" data="rfc4915"/>
    <range>
      <value>0-2</value>
      <registration_rule>Reserved</registration_rule>
      <note>IANA does not assign</note>
    </range>
    <range>
      <value>3-31</value>
      <registration_rule>IETF Review</registration_rule>
    </range>
    <range>
      <value>32-127</value>
      <registration_rule>Reserved</registration_rule>
      <note>IANA does not assign</note>
    </range>
    <range>
      <value>128-255</value>
      <registration_rule>Reserved</registration_rule>
      <note>IANA does not assign</note>
    </range>
    <record>
      <value>0</value>
      <description>Reserved for advertising the metric associated    
with the default topology (see Section 4.2)</description>
      <xref type="rfc" data="rfc4915"/>
    </record>
    <record>
      <value>1</value>
      <description>Reserved for advertising the metric associated    
with the default multicast topology</description>
      <xref type="rfc" data="rfc4915"/>
    </record>
    <record>
      <value>2</value>
      <description>Reserved for IPv4 in-band management purposes</description>
      <xref type="rfc" data="rfc4915"/>
    </record>
    <record>
      <value>3-31</value>
      <description>Unassigned</description>
      <xref type="rfc" data="rfc4915"/>
    </record>
    <record>
      <value>32-127</value>
      <description>Reserved for development, experimental and        
proprietary features</description>
      <xref type="rfc" data="rfc4915"/>
      <xref type="rfc" data="rfc3692"/>
    </record>
    <record>
      <value>128-255</value>
      <description>Invalid and SHOULD be ignored</description>
      <xref type="rfc" data="rfc4915"/>
    </record>
  </registry>
  <people/>
</registry>
