    <record>
      <value>0</value>
      <description>GRE</description>
      <xref type="rfc" data="rfc2784"/>     
    </record>
    <record>
      <value>1</value>
      <description>Enhanced GRE</description>
      <xref type="rfc" data="rfc2637"/>     
    </record>
  </registry>
