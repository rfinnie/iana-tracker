<registry xmlns="http://www.iana.org/assignments" id="rpcbind-transport-parameters">
  <title>RPCBIND Transport Parameters</title>
  <category>Remote Procedure Call (RPC)</category>
  <created>2006-11-08</created>
  <updated>2011-10-06</updated>
  <registry id="rpcbind-transport-parameters-1">
    <title>Transport semantics (r_nc_semantics)</title>
    <category>Remote Procedure Call (RPC)</category>
    <xref type="rfc" data="rfc1833"/>
    <registration_rule>Standards Action</registration_rule>
    <note>This represents the type of transport, and has the following values.</note>
    <record>
      <value>1</value>
      <name>NC_TPI_CLTS</name>
      <description>Connectionless</description>
      <xref type="rfc" data="rfc1833"/>
    </record>
    <record>
      <value>2</value>
      <name>NC_TPI_COTS</name>
      <description>Connection oriented</description>
      <xref type="rfc" data="rfc1833"/>
    </record>
    <record>
      <value>3</value>
      <name>NC_TPI_COTS_ORD</name>
      <description>Connection oriented with graceful close</description>
      <xref type="rfc" data="rfc1833"/>
    </record>
    <record>
      <value>4</value>
      <name>NC_TPI_RAW</name>
      <description>Raw transport</description>
      <xref type="rfc" data="rfc1833"/>
    </record>
  </registry>
