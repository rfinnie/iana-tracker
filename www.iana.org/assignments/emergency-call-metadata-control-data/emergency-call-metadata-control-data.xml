<registry xmlns="http://www.iana.org/assignments" id="emergency-call-metadata-control-data">
  <title>Emergency Call Metadata/Control Data</title>
  <created>2017-02-24</created>
  <updated>2017-05-18</updated>
  
  <registry id="emergency-call-action">
    <title>Emergency Call Actions</title>
    <xref type="rfc" data="rfc8147"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Randall Gellens</expert>
    <record date="2017-02-24">
      <value>send-data</value>
      <description>See Section 9.1.3.1 of <xref type="rfc" data="rfc8147"/></description>
      <xref type="rfc" data="rfc8147"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>msg-static</value>
      <description>See Section 9.1 of <xref type="rfc" data="rfc8148"/></description>
      <xref type="rfc" data="rfc8148"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>msg-dynamic</value>
      <description>See Section 9.1 of <xref type="rfc" data="rfc8148"/></description>
      <xref type="rfc" data="rfc8148"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>honk</value>
      <description>See Section 9.1 of <xref type="rfc" data="rfc8148"/></description>
      <xref type="rfc" data="rfc8148"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>lamp</value>
      <description>See Section 9.1 of <xref type="rfc" data="rfc8148"/></description>
      <xref type="rfc" data="rfc8148"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>enable-camera</value>
      <description>See Section 9.1 of <xref type="rfc" data="rfc8148"/></description>
      <xref type="rfc" data="rfc8148"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>door-lock</value>
      <description>See Section 9.1 of <xref type="rfc" data="rfc8148"/></description>
      <xref type="rfc" data="rfc8148"/>
      <controller>IESG</controller>
    </record>
  </registry>
  
  <registry id="emergency-call-action-failure-reason">
    <title>Emergency Call Action Failure Reasons</title>
    <xref type="rfc" data="rfc8147"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Randall Gellens</expert>
    <record date="2017-02-24">
      <value>damaged</value>
      <description>Required components are damaged.</description>
      <xref type="rfc" data="rfc8147"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-24">
      <value>data-unsupported</value>
      <description>The data item referenced in a "send-data" 
        request is not supported.</description>
      <xref type="rfc" data="rfc8147"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-24">
      <value>security-failure</value>
      <description>The authenticity of the request or the 
        authority of the requestor could not be verified.</description>
      <xref type="rfc" data="rfc8147"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-24">
      <value>unable</value>
      <description>The action could not be accomplished (a 
        generic error for use when no other code is appropriate).</description>
      <xref type="rfc" data="rfc8147"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-24">
      <value>unsupported</value>
      <description>The "action" value is not supported.</description>
      <xref type="rfc" data="rfc8147"/>
      <controller>IESG</controller>
    </record>
  </registry>
  
  <registry id="emergency-call-static-message">
    <title>Emergency Call Static Messages</title>
    <xref type="rfc" data="rfc8148"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Randall Gellens</expert>
    <record date="2017-03-01">
      <value>0</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc8148"/>
    </record>
    <record date="2017-02-28">
      <value>1</value>
      <description>Emergency services has received your information and 
        location but cannot speak with you right now.  We will get 
        help to you as soon as possible.</description>
      <xref type="rfc" data="rfc8148"/>
    </record>
  </registry>
  
