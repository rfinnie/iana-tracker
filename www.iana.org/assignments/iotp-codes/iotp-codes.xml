  <created>2000-04</created>
  <updated>2012-02-02</updated>
  <expert>David Burdett</expert>
  <registry id="iotp-codes-1">
    <title>Algorithm/Name</title>
    <xref type="rfc" data="rfc2801"/>
    <registration_rule>Expert Review</registration_rule>
    <note>When Algorithm is a child of an AuthReq Component.
Further information see Section 12.1 in the RFC.</note>
    <record>
      <value>"sha1"</value>
      <description>Indicates that a [SHA1] authentication will apply</description>
      <xref type="rfc" data="rfc2801"/>
    </record>
    <record>
      <value>"signature"</value>
      <description>Indicates that authentication consists of the generation       
of a digital signature.</description>
      <xref type="rfc" data="rfc2801"/>
    </record>
    <record>
      <value>"Pay:ppp"</value>
      <description>Where "ppp" may be set to any valid value for "iotpbrand"</description>
      <xref type="rfc" data="rfc2801"/>
    </record>
  </registry>
