<registry xmlns="http://www.iana.org/assignments" id="multicast-ping">
  <title>Multicast Ping Protocol Parameters</title>
  <created>2011-10-31</created>
  <updated>2011-12-09</updated>
  
  <registry id="message-types">
    <title>Multicast Ping Protocol Message Types</title>
    <xref type="rfc" data="rfc6450"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Unassigned</expert>
    <record>
      <type>0-64</type>
      <name>Unassigned</name>
    </record>
    <record>
      <type>65</type>
      <name>Echo Reply</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>66-72</type>
      <name>Unassigned</name>
    </record>
    <record>
      <type>73</type>
      <name>Init</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>74-80</type>
      <name>Unassigned</name>
    </record>
    <record>
      <type>81</type>
      <name>Echo Request</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>82</type>
      <name>Unassigned</name>
    </record>
    <record>
      <type>83</type>
      <name>Server Response</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>84-253</type>
      <name>Unassigned</name>
    </record>
    <record>
      <type>254-255</type>
      <name>Experimental</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
  </registry>
  
  <registry id="option-types">
    <title>Multicast Ping Protocol Option Types</title>
    <xref type="rfc" data="rfc6450"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Unassigned</expert>
    <record>
        <type>0</type>
        <name>Version</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>1</type>
      <name>Client ID</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>2</type>
      <name>Sequence Number</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>3</type>
      <name>Client Timestamp</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>4</type>
      <name>Multicast Group</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>5</type>
      <name>Option Request</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>6</type>
      <name>Server Information</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>7</type>
      <name>Deprecated</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>8</type>
      <name>Deprecated</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>9</type>
      <name>TTL</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>10</type>
      <name>Multicast Prefix</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>11</type>
      <name>Session ID</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>12</type>
      <name>Server Timestamp</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
    <record>
      <type>13-65531</type>
      <name>Unassigned</name>
    </record>
    <record>
      <type>65532-65535</type>
      <name>Experimental</name>
      <xref type="rfc" data="rfc6450"/>
    </record>
  </registry>
  
  <people/>
</registry>
