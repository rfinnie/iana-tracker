        <xref type="rfc" data="rfc2909"/>
      </record>
      <record>
        <value>17</value>
        <name>Claim Lifetime Too Short</name>
        <obit>(CC)</obit>
        <xref type="rfc" data="rfc2909"/>
      </record>
      <record>
        <value>18</value>
        <name>Claim Lifetime Too Long</name>
