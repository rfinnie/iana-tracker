<registry xmlns="http://www.iana.org/assignments" id="dnskey-flags">
  <title>DNSKEY FLAGS</title>
  <category>Domain Name System (DNS) Parameters</category>
  <created>2004-02-04</created>
  <updated>2007-09-05</updated>
  <registry id="dnskey-flags-1">
    <title>DNSKEY RR Flags</title>
    <xref type="rfc" data="rfc3755"/>
    <xref type="rfc" data="rfc4034"/>
    <registration_rule>Standards Action</registration_rule>
    <note>The DNSKEY RR contains a 16-bit flags field.  The KEY RR may have different
flag assignments -- those assignments are not recorded in this registry.</note>
    <record>
      <value>0-6</value>
      <description>Unassigned</description>
      <xref type="rfc" data="rfc3755"/>
      <xref type="rfc" data="rfc4034"/>
    </record>
    <record>
      <value>7</value>
      <description>ZONE</description>
      <xref type="rfc" data="rfc3755"/>
      <xref type="rfc" data="rfc4034"/>
    </record>
    <record>
      <value>8</value>
      <description>REVOKE</description>
      <xref type="rfc" data="rfc5011"/>
    </record>
    <record>
      <value>9-14</value>
      <description>Unassigned</description>
      <xref type="rfc" data="rfc3755"/>
      <xref type="rfc" data="rfc4034"/>
    </record>
    <record>
      <value>15</value>
      <description>Secure Entry Point (SEP)</description>
      <xref type="rfc" data="rfc3757"/>
      <xref type="rfc" data="rfc4034"/>
    </record>
  </registry>
  <people/>
</registry>
