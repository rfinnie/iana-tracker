<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="pkix-parameters.xsl"?>
<?oxygen RNGSchema="pkix-parameters.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="pkix-parameters">
  <title>Public Key Infrastructure using X.509 (PKIX) Parameters</title>
  <created>2003-12-23</created>
  <updated>2013-03-18</updated>
  
  <registry id="srv-protocol-labels">
    <title>PKIX SRV Protocol Labels</title>
    <xref type="rfc" data="rfc4386"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Unassigned</expert>
    <record>
      <name>_PKIXREP</name>
      <xref type="rfc" data="rfc4386"/>
    </record>
  </registry>
  
  <registry id="caa-properties">
    <title>Certification Authority Restriction Properties</title>
    <xref type="rfc" data="rfc6844"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Unassigned</expert>
    <record>
      <name>issue</name>
      <description>Authorization Entry by Domain</description>
      <xref type="rfc" data="rfc6844"/>
    </record>
    <record>
      <name>issuewild</name>
      <description>Authorization Entry by Wildcard Domain</description>
      <xref type="rfc" data="rfc6844"/>
    </record>
    <record>
      <name>iodef</name>
      <description>Report incident by IODEF report</description>
      <xref type="rfc" data="rfc6844"/>
    </record>
    <record>
      <name>auth</name>
      <description>Reserved</description>
      <xref type="rfc" data="rfc6844"/><xref type="rfc-errata" data="3547"/>
    </record>
    <record>
      <name>path</name>
      <description>Reserved</description>
      <xref type="rfc" data="rfc6844"/><xref type="rfc-errata" data="3547"/>
    </record>
    <record>
      <name>policy</name>
      <description>Reserved</description>
      <xref type="rfc" data="rfc6844"/><xref type="rfc-errata" data="3547"/>
    </record>
  </registry>
  
  <registry id="caa-flags">
    <title>Certification Authority Restriction Flags</title>
    <xref type="rfc" data="rfc6844"/>
    <registration_rule>RFC Required</registration_rule>
    <record>
      <value>0</value>
      <description>Issuer Critical Flag</description>
      <xref type="rfc" data="rfc6844"/>
    </record>
    <record>
      <value>1-7</value>
      <description>Unassigned</description>
      <xref type="rfc" data="rfc6844"/><xref type="rfc-errata" data="3532"/>
    </record>
  </registry>
  
  <people/>
</registry>
