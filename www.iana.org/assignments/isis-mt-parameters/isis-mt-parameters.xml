<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="isis-mt-parameters.xsl"?>
<?xml-model href="isis-mt-parameters.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="isis-mt-parameters">
  <title>IS-IS Multi-Topology Parameters</title>
  <category>IS-IS</category>
  <created>2007-11-20</created>
  <updated>2017-06-23</updated>
  <note>For IS-IS registries and value ranges maintained via the "Expert Review" <xref type="rfc" data="rfc8126"/> 
registration procedure, guidance for IESG-designated experts can be found in 
<xref type="rfc" data="rfc7370"/>.
  </note>
  
  <registry id="isis-mt-parameters-2">
    <title>IS-IS Multi-Topology ID Values</title>
    <xref type="rfc" data="rfc5120"/>
    <expert>Chris Hopps, Hannes Gredler, Les Ginsberg</expert>
    <range>
      <value>0-3995</value>
      <registration_rule>Expert Review</registration_rule>
    </range>
    <range>
      <value>3996-4095</value>
      <registration_rule>Private Use</registration_rule>
    </range>
    <record>
      <value>0</value>
      <description>Equivalent to the "standard" topology</description>
      <xref type="rfc" data="rfc5120"/>
    </record>
    <record>
      <value>1</value>
      <description>Reserved for IPv4 in-band management purposes</description>
      <xref type="rfc" data="rfc5120"/>
    </record>
    <record>
      <value>2</value>
      <description>Reserved for IPv6 routing topology</description>
      <xref type="rfc" data="rfc5120"/>
    </record>
    <record>
      <value>3</value>
      <description>Reserved for IPv4 multicast routing topology</description>
      <xref type="rfc" data="rfc5120"/>
    </record>
    <record>
      <value>4</value>
      <description>Reserved for IPv6 multicast routing topology</description>
      <xref type="rfc" data="rfc5120"/>
    </record>
    <record>
      <value>5</value>
      <description>Reserved for IPv6 in-band management purposes</description>
      <xref type="rfc" data="rfc5120"/>
    </record>
    <record>
      <value>6-3995</value>
      <description>Unassigned</description>
    </record>
    <record>
      <value>3996-4095</value>
      <description>Reserved for Private Use</description>
      <xref type="rfc" data="rfc5120"/>
    </record>
  </registry>
  <people/>
</registry>
