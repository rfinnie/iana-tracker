<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="dsn-types.xsl"?>
<?oxygen RNGSchema="dsn-types.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="dsn-types">
  <title>Delivery Status Notification (DSN) Types</title>
  <created>2002-11-07</created>
  <updated>2016-05-19</updated>

  

  <registry id="dsn-types-1">
    <title>Address Types</title>
    <xref type="rfc" data="rfc3461"/>
    <xref type="rfc" data="rfc3464"/>
    <registration_rule>RFC Required</registration_rule>
    <record>
      <type>rfc822</type>
      <description>Internet Mail</description>
      <xref type="rfc" data="rfc3461"/>
    </record>
    <record>
      <type>x400</type>
      <description>X.400 Mail</description>
      <xref type="rfc" data="rfc2156"/>
    </record>
    <record>
      <type>UTF-8</type>
      <description>UTF-8 mail address</description>
      <xref type="rfc" data="rfc6533"/>
    </record>
  </registry>

  

  <registry id="dsn-types-2">
    <title>Diagnostic Types</title>
    <xref type="rfc" data="rfc3461"/>
    <xref type="rfc" data="rfc3464"/>
    <registration_rule>RFC Required</registration_rule>
    <record>
      <type>smtp</type>
      <description>Internet Mail</description>
      <xref type="rfc" data="rfc3461"/>
      <xref type="rfc" data="rfc3464"/>
      <xref type="rfc" data="rfc6533"/>
    </record>
  </registry>

  

  <registry id="dsn-types-3">
    <title>MTA Name Types</title>
    <xref type="rfc" data="rfc3461"/>
    <xref type="rfc" data="rfc3464"/>
    <registration_rule>RFC Required</registration_rule>
    <record>
      <type>dns</type>
      <description>Internet Mail</description>
      <xref type="rfc" data="rfc3461"/>
    </record>
    <record date="2016-05-18">
      <type>X.400 Mail</type>
      <description>X.400 Mail</description>
      <xref type="rfc" data="rfc2156"/>
    </record>
  </registry>
  <people/>
</registry>
