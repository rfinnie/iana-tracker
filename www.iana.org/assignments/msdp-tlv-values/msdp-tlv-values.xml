<registry xmlns="http://www.iana.org/assignments" id="msdp-tlv-values">
  <title>Multicast Source Discovery Protocol (MSDP) TLV Values Registry</title>
  <created>2003-09-17</created>
  <updated>2003-10-31</updated>
  <registry id="msdp-tlv-values-2">
    <title>MSDP TLV Values</title>
    <xref type="rfc" data="rfc3618"/>
    <range>
      <value>1-200</value>
      <registration_rule>IESG Approval or Standards Action Process</registration_rule>
    </range>
    <range>
      <value>201-255</value>
      <registration_rule>Experimental use</registration_rule>
    </range>
    <record>
      <value>1</value>
      <description>IPv4 Source-Active</description>
      <xref type="rfc" data="rfc3618"/>
    </record>
    <record>
      <value>2</value>
      <description>IPv4 Source-Active Request</description>
      <xref type="rfc" data="rfc3618"/>
    </record>
    <record>
      <value>3</value>
      <description>IPv4 Source-Active Response</description>
      <xref type="rfc" data="rfc3618"/>
    </record>
    <record>
      <value>4</value>
      <description>KeepAlive</description>
      <xref type="rfc" data="rfc3618"/>
    </record>
    <record>
      <value>5</value>
      <description>Reserved (Previously: Notification)</description>
      <xref type="rfc" data="rfc3618"/>
    </record>
    <record>
      <value>6</value>
      <description>MSDP traceroute in progress</description>
      <xref type="rfc" data="rfc3618"/>
    </record>
    <record>
      <value>7</value>
      <description>MSDP traceroute reply</description>
      <xref type="rfc" data="rfc3618"/>
    </record>
    <record>
      <value>8-200</value>
      <description>Unassigned</description>
    </record>
    <record>
      <value>201-255</value>
      <description>Experimental Use</description>
      <xref type="rfc" data="rfc3618"/>
    </record>
  </registry>
  <people/>
</registry>
