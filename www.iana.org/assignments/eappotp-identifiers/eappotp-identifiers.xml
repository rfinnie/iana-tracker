    </record>
    <record>
      <octet_1>0x80</octet_1>
      <octet_2>-</octet_2>
      <hash_algorithm>Vendor-specific (or experimental)</hash_algorithm>
      <xref type="rfc" data="rfc4793"/>
    </record>
