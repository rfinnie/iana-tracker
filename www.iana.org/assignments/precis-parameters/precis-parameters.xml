<?xml-stylesheet type="text/xsl" href="precis-parameters.xsl"?>
<?oxygen RNGSchema="precis-parameters.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="precis-parameters">
  <title>Preparation and Comparison of Internationalized Strings (PRECIS) Parameters</title>
  <created>2015-03-06</created>
  <updated>2015-12-17</updated>
  
  <registry id="base-classes">
    <title>PRECIS Base Classes</title>
    <xref type="rfc" data="rfc7564"/>
    <registration_rule>RFC Required</registration_rule>
    <record date="2015-03-06">
      <value>FreeformClass</value>
      <description>A sequence of letters, numbers, symbols, spaces, and 
        other code points that is used for free-form strings.</description>
      <xref type="rfc" data="rfc7564">RFC7564, Section 4.3</xref> 
    </record>
    <record date="2015-03-06">
      <value>IdentifierClass</value>
      <description>A sequence of letters, numbers, and symbols that is 
        used to identify or address a network entity.</description>
      <xref type="rfc" data="rfc7564">RFC7564, Section 4.2</xref> 
    </record>
  </registry>

  <registry id="profiles">
    <title>PRECIS Profiles</title>
    <xref type="rfc" data="rfc7564"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Peter Saint-Andre (primary), Pete Resnick (secondary), Andrew Sullivan (backup)</expert>
    <record date="2015-06-08">
      <name>UsernameCaseMapped</name>
      <class>IdentifierClass</class>
      <replaces>The SASLprep profile of stringprep</replaces>
      <file type="template">profiles/UsernameCaseMapped.txt</file>
      <xref type="rfc" data="rfc7613">RFC7613, Section 3.2</xref>
    </record>
    <record date="2015-06-08">
      <name>UsernameCasePreserved</name>
      <class>IdentifierClass</class>
      <replaces>The SASLprep profile of stringprep</replaces>
      <file type="template">profiles/UsernameCasePreserved.txt</file>
      <xref type="rfc" data="rfc7613">RFC7613, Section 3.3</xref>
    </record>
    <record date="2015-06-08">
      <name>OpaqueString</name>
      <class>FreeformClass</class>
      <replaces>The SASLprep profile of stringprep</replaces>
      <file type="template">profiles/OpaqueString.txt</file>
      <xref type="rfc" data="rfc7613">RFC7613, Section 4.2</xref>
    </record>
    <record date="2015-09-11">
      <name>Nickname</name>
      <class>FreeformClass</class>
      <replaces>None</replaces>
      <file type="template">profiles/Nickname.txt</file>
      <xref type="rfc" data="rfc7700">RFC7700</xref>
    </record>
  </registry>


  <people/>
</registry>
