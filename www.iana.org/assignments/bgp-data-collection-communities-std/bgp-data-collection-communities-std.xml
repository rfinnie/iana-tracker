  <registry id="bgp-data-collection-communities-std-1">
    <title>BGP Data Collection Standard Communities</title>
    <xref type="rfc" data="rfc4384"/>
    <registration_rule>IETF Consensus</registration_rule>
    <note>usually through the Global Routing Operations Working Group (grow)</note>
    <record>
      <value>0000000000000000</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc4384"/>
    </record>
    <record>
      <value>0000000000000001</value>
      <description>Customer Routes</description>
      <xref type="rfc" data="rfc4384"/>
    </record>
    <record>
      <value>0000000000000010</value>
      <description>Peer Routes</description>
      <xref type="rfc" data="rfc4384"/>
    </record>
    <record>
      <value>0000000000000011</value>
      <description>Internal Routes</description>
      <xref type="rfc" data="rfc4384"/>
    </record>
    <record>
      <value>0000000000000100</value>
      <description>Internal More Specific Routes</description>
      <xref type="rfc" data="rfc4384"/>
    </record>
    <record>
      <value>0000000000000101</value>
      <description>Special Purpose Routes</description>
      <xref type="rfc" data="rfc4384"/>
    </record>
    <record>
      <value>0000000000000110</value>
      <description>Upstream Routes</description>
      <xref type="rfc" data="rfc4384"/>
