  <registry id="bgpsec-capability">
    <title>BGPsec Capability</title>
    <xref type="draft" data="RFC-ietf-sidr-bgpsec-protocol-22"/>
    <registration_rule>Standards Action</registration_rule>
    <record date="2017-01-26">
      <value>0-3</value>
      <description>Version<br/>
        Value = 0x0</description>
      <xref type="draft" data="RFC-ietf-sidr-bgpsec-protocol-22"/>
    </record>
    <record date="2017-01-26">
      <value>4</value>
      <description>Direction<br/>
        (Both possible values 0 and 1 are fully specified by <xref type="draft" data="RFC-ietf-sidr-bgpsec-protocol-22"/>)</description>
      <xref type="draft" data="RFC-ietf-sidr-bgpsec-protocol-22"/>
    </record>
    <record date="2017-01-26">
      <value>5-7</value>
      <description>Unassigned<br/>
        Value = 000 (in binary)</description>
      <xref type="draft" data="RFC-ietf-sidr-bgpsec-protocol-22"/>
    </record>
