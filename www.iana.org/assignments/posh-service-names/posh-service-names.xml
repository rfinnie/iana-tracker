<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="posh-service-names.xsl"?>
<?xml-model href="posh-service-names.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="posh-service-names">
<category>Uniform Resource Identifier (URI) Schemes</category>
  <title>POSH Service Names</title>
  <created>2015-09-20</created>
  <updated>2015-11-24</updated>

  <registry id="service-names">
    <title>POSH Service Names</title>
    <xref type="rfc" data="rfc7711"/>
    <registration_rule>Expert Review or IETF Review</registration_rule>
    <expert>Unassigned</expert>
    <record date="2015-09-29">
      <value>xmpp-client</value>
      <controller>IETF</controller>
      <description>Specifies the location of a POSH file 
        containing verification material or a reference thereto that enables 
        a client to verify the identity of a server for a client-to-server 
        stream in XMPP</description>
      <xref type="rfc" data="rfc7712"/>
    </record>
    <record date="2015-09-29">
      <value>xmpp-server</value>
      <controller>IETF</controller>
      <description>Specifies the location of a POSH file containing 
        verification material or a reference thereto that enables a server 
        to verify the identity of a peer server for a server-to-server 
        stream in XMPP</description>
      <xref type="rfc" data="rfc7712"/>
    </record>
  </registry>
  <people/>
</registry>
