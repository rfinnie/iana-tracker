<?oxygen RNGSchema="geo-uri-parameters.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="geo-uri-parameters">
  <title>'geo' URI Parameters</title>
  <category>Uniform Resource Identifier (URI) Schemes</category>
  <created>2010-04-20</created>
  <updated>2010-06-07</updated>

  <registry id="geo-uri-parameters-1">
    <title>'geo' URI Parameters</title>
    <xref type="rfc" data="rfc5870"/>
    <registration_rule>Specification Required</registration_rule>   
    
    <record>
      <code>crs</code>
      <profile>Predefined</profile>
      <xref type="rfc" data="rfc5870"/>
    </record>
    <record>
      <code>u</code>
      <profile>Constrained</profile>
      <xref type="rfc" data="rfc5870"/>
    </record>

  </registry>
  
  
  
  <registry id="geo-uri-parameters-2">
    <title>'geo' URI 'crs' Parameter Values</title>
    <xref type="rfc" data="rfc5870"/>
    <registration_rule>IESG Approval with Specification Required</registration_rule>

    <record>
      <code>wgs84</code>
      <algorithm>urn:ogc:def:crs:EPSG::4326</algorithm>
      <xref type="rfc" data="rfc5870"/>
    </record>
    <record>
      <code>wgs84</code>
      <algorithm>urn:ogc:def:crs:EPSG::4979</algorithm>
      <xref type="rfc" data="rfc5870"/>
    </record>

  </registry>
  <people/>
</registry>
