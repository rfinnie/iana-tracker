<registry xmlns="http://www.iana.org/assignments" id="message-store-events">
  <title>Internet Message Store Events</title>
  <created>2008-12-04</created>
  <updated>2009-03-27</updated>
  <registry id="message-store-events-2">
    <title>Event Names</title>
    <xref type="rfc" data="rfc5423"/>
    <range>
      <value>name starts wth "vnd."; intended for private use</value>
      <registration_rule>First Come First Served</registration_rule>
    </range>
    <range>
      <value>name doesn't start with "vnd."; intended for interoperable use</value>
      <registration_rule>IETF Review</registration_rule>
    </range>
    <record>
      <description>FlagsClear</description>
