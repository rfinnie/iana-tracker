      <xref type="rfc" data="rfc8392"/>
    </record>
    <record date="2018-03-22">
      <claim>iat</claim>
      <description>Issued At</description>
      <jwt>iat</jwt>
      <key>6</key>
      <value>integer or floating-point number</value>
      <change><xref type="person" data="IESG"/></change>
      <xref type="rfc" data="rfc8392"/>
