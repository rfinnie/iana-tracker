    <record date="2018-03-22">
      <claim>cti</claim>
      <description>CWT ID</description>
      <jwt>jti</jwt>
      <key>7</key>
      <value>byte string</value>
      <change><xref type="person" data="IESG"/></change>
      <xref type="rfc" data="rfc8392"/>
    </record>
    <record date="2019-11-08">
      <claim>cnf</claim>
      <description>Confirmation</description>
      <jwt>cnf</jwt>
