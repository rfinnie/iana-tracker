<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="mdn.xsl"?>
<?oxygen RNGSchema="mdn.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="mdn">
  <title>Message Disposition Notification (MDN) Parameters</title>
  <updated>2012-09-18</updated>
  <registry id="disp-mod">
    <title>MDN-Disposition-modifier-names</title>
    <xref type="rfc" data="rfc3798"/>
    <registration_rule>Specification required</registration_rule>
    <expert>Tony Hansen</expert>
    <record>
      <value>Alternative-preferred</value>
      <xref type="rfc" data="rfc3297"/>
    </record>
    <record>
      <value>Original-lost</value>
      <xref type="rfc" data="rfc3297"/>
    </record>
    <record>
      <value>warning</value>
      <xref type="rfc" data="rfc4130"/>
    </record>
  </registry>
  <registry id="disp-not-opt">
    <title>MDN-Disposition-notification-option-names</title>
    <xref type="rfc" data="rfc3798"/>
    <registration_rule>Specification required</registration_rule>
    <record>
      <value>Alternative-available</value>
      <xref type="rfc" data="rfc3297"/>
    </record>
    <record>
      <value>Alternative-not-available</value>
      <xref type="rfc" data="rfc3297"/>
    </record>
    <record>
      <value>signed-receipt-protocol</value>
      <xref type="rfc" data="rfc3335"/>
    </record>
    <record>
      <value>signed-receipt-micalg</value>
      <xref type="rfc" data="rfc3335"/>
    </record>
  </registry>
  <registry id="ext-field-names">
    <title>MDN-Extension-field-names</title>
    <xref type="rfc" data="rfc3798"/>
    <registration_rule>Specification required</registration_rule>
    <record>
      <value>Received-content-MIC</value>
      <description>Message Integrity Check value of sent document</description>
      <xref type="rfc" data="rfc3335"/>
    </record>
  </registry>
  <people/>
</registry>
