<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="calipso.xsl"?>
<?oxygen RNGSchema="calipso.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="calipso">
  <title>Common Architecture Label IPv6 Security Option (CALIPSO)</title>
  <created>2009-04-24</created>
  <updated>2009-07-14</updated>
  <registry id="calipso-2">
    <title>CALIPSO Domain of Interpretation (DOI)</title>
    <xref type="rfc" data="rfc5570"/>
    <expert>Unassigned</expert>
    <range>
      <value>1:0:0:0 - 254:255:255:255</value>
      <registration_rule>Expert Review</registration_rule>
    </range>
    <range>
      <value>255:0:0:0 - 255:255:255:255</value>
      <registration_rule>IESG Approval</registration_rule>
    </range>
    <record>
      <value>0:0:0:0</value>
      <description>Null DOI.  This ought not be used on any network.</description>
      <xref type="rfc" data="rfc5570"/>
    </record>
    <record>
      <value>0:0:0:1 - 0:255:255:255</value>
      <description>For private use among consenting parties within private networks</description>
      <xref type="rfc" data="rfc5570"/>
    </record>
    <record>
      <value>1:0:0:0 - 254:255:255:255</value>
      <description>Unassigned</description>
    </record>
    <record>
      <value>255:0:0:0 - 255:255:255:255</value>
      <description>Reserved to the IETF for future use</description>
      <xref type="rfc" data="rfc5570"/>
    </record>
  </registry>
</registry>
