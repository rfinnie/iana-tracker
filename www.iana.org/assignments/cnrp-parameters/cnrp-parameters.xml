<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="cnrp-parameters.xsl"?>
<?xml-model href="cnrp-parameters.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="cnrp-parameters">
  <title>Common Name Resolution Protocol (CNRP) Parameters</title>
  <created>2002-08-15</created>
  <updated>2002-08-21</updated>
  <registry id="cnrp-parameters-1">
    <title>Properties</title>
    <xref type="rfc" data="rfc3367"/>
    <registration_rule>First Come First Served</registration_rule>
    <record>
      <name>geography</name>
      <type>iso3166-1</type>
      <description>A geographic location</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <name>language</name>
      <type>rfc1766</type>
      <description>A language specification</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <name>category</name>
      <type>freeform</type>
      <description>A node in some system of semantic           
relationships that is considered relevant
to the common-name.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <name>range</name>
      <type>range</type>
      <description>A range given in the format "x,y" where     
x is the starting point and y is the
length.  This property is used by the
client to tell the server that is is
requesting a subrange of the results.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <name>dataseturi</name>
      <type>uri</type>
      <description>A URI used to disambiguate between two      
Datasets offered by the same Service.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
  </registry>
  <registry id="cnrp-parameters-2">
    <title>Property Types</title>
    <xref type="rfc" data="rfc3367"/>
    <registration_rule>First Come First Served</registration_rule>
    <record>
      <type>freeform</type>
      <property>category</property>
      <description>The value is to be interpreted by the       
server the best way it knows how.  This
value has no defined structure.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <type>freeform</type>
      <property>geography</property>
      <description>The value is to be interpreted by the       
server the best way it knows how.  This
value has no defined structure.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <type>freeform</type>
      <property>language</property>
      <description>The value is to be interpreted by the       
server the best way it knows how.  This
value has no defined structure.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <type>iso3166-2</type>
      <property>geography</property>
      <description>The combination of country and sub-region   
codes found in ISO 3166-2.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <type>iso3166-1</type>
      <property>Geography</property>
      <description>Country Codes found in ISO 3166-1.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <type>postalcode</type>
      <property>Geography</property>
      <description>A postal code that is valid for some        
region.  A good example is the Zip code
system used in the US.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <type>lat-long</type>
      <property>Geography</property>
      <description>The latitude and longitude of a             
geographical location in the form specified
in Department of Commerce, 1986,
Representation of geographic point locations
for information interchange (Federal
Information Processing Standard 70-1):
Washington,  Department of Commerce, National
Institute of Standards and Technology.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <type>rfc1766</type>
      <property>Language</property>
      <description>Language codes as defined by RFC 1766.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <type>naics</type>
      <property>Category</property>
      <description>North American Industry Code System.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
    <record>
      <type>uri</type>
      <property>dataseturi</property>
      <description>A URI adhering to the 'absoluteURI'         
production of the Collected ABNF found
in RFC 2396.</description>
      <xref type="rfc" data="rfc3367"/>
    </record>
  </registry>
  <registry id="cnrp-parameters-3">
    <title>Status Messages</title>
    <xref type="rfc" data="rfc3367"/>
    <registration_rule>First Come First Served</registration_rule>
    <registry id="cnrp-parameters-4">
      <title>Status Messages Level 1 - (Informative) Codes</title>
      <xref type="rfc" data="rfc3367"/>
      <registration_rule>First Come First Served</registration_rule>
      <record>
        <code>1.0.0</code>
        <description>Undefined Information</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>1.1.0</code>
        <description>Query related information</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>1.2.0</code>
        <description>An informative message pertaining to the Service</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
    </registry>
    <registry id="cnrp-parameters-5">
      <title>Status Messages Level 2 - (Success) Codes</title>
      <xref type="rfc" data="rfc3367"/>
      <registration_rule>First Come First Served</registration_rule>
      <record>
        <code>2.0.0</code>
        <description>Something undefined succeeded</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>2.1.0</code>
        <description>Query succeeded</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
    </registry>
    <registry id="cnrp-parameters-6">
      <title>Status Messages Level 3 - (Partial Success) Codes</title>
      <xref type="rfc" data="rfc3367"/>
      <registration_rule>First Come First Served</registration_rule>
      <record>
        <code>3.0.0</code>
        <description>Something undefined was only partially successful</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>3.1.0</code>
        <description>The query was only partially successful</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>3.1.1</code>
        <description>The query contained invalid or unsupported properties</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>3.1.2</code>
        <description>The XML was well formed but invalid</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>3.1.3</code>
        <description>Server does not support datasets</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>3.1.4</code>
        <description>The first dataset in the list of datasets you gave in the    
query was the only one used</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>3.1.5</code>
        <description>This dataset not supported</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>3.2.0</code>
        <description>The server caused a partially successful event</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>3.2.1</code>
        <description>Some referral server was unavailable</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
    </registry>
    <registry id="cnrp-parameters-7">
      <title>Status Messages Level 4 - (Transient Failure) Codes</title>
      <xref type="rfc" data="rfc3367"/>
      <registration_rule>First Come First Served</registration_rule>
      <record>
        <code>4.0.0</code>
        <description>Something undefined caused a persistent transient failure</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>4.1.0</code>
        <description>There was an error in the query that made it unable to be    
interpreted</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>4.2.0</code>
        <description>The query was to complex</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>4.2.1</code>
        <description>The Service was too busy</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>4.2.2</code>
        <description>The Server is in maintenance</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>4.2.3</code>
        <description>The Server had an internal error</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
    </registry>
    <registry id="cnrp-parameters-8">
      <title>Status Messages Level 5 - (Permanent Failures) Codes</title>
      <xref type="rfc" data="rfc3367"/>
      <registration_rule>First Come First Served</registration_rule>
      <record>
        <code>5.0.0</code>
        <description>Something undefined caused a permanent failure</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>5.1.0</code>
        <description>The query permanently failed</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>5.2.0</code>
        <description>The service had a permanent failure</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>5.2.1</code>
        <description>This Service is no longer available</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
      <record>
        <code>5.2.2</code>
        <description>The Server had a permanent failure</description>
        <xref type="rfc" data="rfc3367"/>
      </record>
    </registry>
  </registry>
  <people/>
</registry>
