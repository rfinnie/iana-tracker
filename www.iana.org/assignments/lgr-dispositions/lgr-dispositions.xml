<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="lgr-dispositions.xsl"?>
<?oxygen RNGSchema="lgr-dispositions.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="lgr-dispositions">
  <title>Label Generation Ruleset (LGR) Dispositions</title>
  <created>2016-05-27</created>
  <updated>2016-08-12</updated>

  <registry id="standard-dispositions">
    <title>Standard Dispositions</title>
    <xref type="rfc" data="rfc7940"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Unassigned</expert>
    <record date="2016-05-27" updated="2016-07-26">
      <value>invalid</value>
      <description>The resulting string is not a valid label. This disposition
        may be assigned implicitly; see Section 7.5 of <xref type="rfc" data="rfc7940"/>.  No variant labels
        should be generated from a variant mapping with this type.</description>
      <controller>IESG</controller>
      <contact/>
      <xref type="rfc" data="rfc7940"/>
    </record>
    <record date="2016-05-27" updated="2016-07-26">
      <value>blocked</value>
      <description>The resulting string is a valid label but should be blocked
        from registration. This would typically apply for a derived
        variant that is undesirable due to having no practical use or
        being confusingly similar to some other label.</description>
      <controller>IESG</controller>
      <contact/>
      <xref type="rfc" data="rfc7940"/>
    </record>
    <record date="2016-05-27" updated="2016-07-26">
      <value>allocatable</value>
      <description>The resulting string should be reserved for use by the
        same operator of the origin string but not automatically
        allocated for use.</description>
      <controller>IESG</controller>
      <contact/>
      <xref type="rfc" data="rfc7940"/>
    </record>
    <record date="2016-05-27" updated="2016-07-26">
      <value>activated</value>
      <description>The resulting string should be activated for use. (This
        is the same as a Preferred Variant <xref type="rfc" data="rfc3743"/>.)</description>
      <controller>IESG</controller>
      <contact/>
      <xref type="rfc" data="rfc7940"/>
    </record>
    <record date="2016-05-27">
      <value>valid</value>
      <description>The resultant string is a valid label.  (This is the typical
        default action if no dispositions are defined.)</description>
      <controller>IESG</controller>
      <contact/>
      <xref type="rfc" data="rfc7940"/>
    </record>
  </registry>

  <registry id="private-dispositions">
    <title>Private Dispositions</title>
    <xref type="rfc" data="rfc7940"/>
    <registration_rule>First Come First Served</registration_rule>

  </registry>

  <people/>
</registry>
