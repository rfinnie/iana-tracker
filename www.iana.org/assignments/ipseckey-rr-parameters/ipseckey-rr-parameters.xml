<?oxygen RNGSchema="ipseckey-rr-parameters.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="ipseckey-rr-parameters">
  <title>IPSECKEY Resource Record Parameters</title>
  <created>2004-12-20</created>
  <updated>2016-10-18</updated>
  
  <registry id="ipseckey-rr-parameters-1">
    <title>Algorithm Type Field</title>
    <xref type="rfc" data="rfc4025"/>
    <registration_rule>IETF Consensus</registration_rule>
    <record>
      <value>0</value>
      <description>No key is present</description>
      <xref type="rfc" data="rfc4025"/>
    </record>
    <record>
      <value>1</value>
      <description>A DSA key is present, in the format defined in <xref type="rfc" data="rfc2536"/></description>
      <xref type="rfc" data="rfc4025"/>
    </record>
    <record>
      <value>2</value>
      <description>A RSA key is present, in the format defined in <xref type="rfc" data="rfc3110"/></description>
      <xref type="rfc" data="rfc4025"/>
    </record>
    <record date="2016-08-11">
      <value>3</value>
      <description>An ECDSA key is present, in the format defined in <xref type="rfc" data="rfc6605"/></description>
      <xref type="rfc" data="rfc8005"/>
    </record>
    <record>
      <value>4-255</value>
      <description>Unassigned</description>
    </record>
  </registry>
  
  <registry id="ipseckey-rr-parameters-2">
    <title>Gateway Type Field</title>
    <xref type="rfc" data="rfc4025"/>
    <registration_rule>Standards Action</registration_rule>
    <record>
      <value>0</value>
      <description>No gateway is present</description>
      <xref type="rfc" data="rfc4025"/>
    </record>
    <record>
      <value>1</value>
      <description>A 4-byte IPv4 address is present</description>
      <xref type="rfc" data="rfc4025"/>
    </record>
    <record>
      <value>2</value>
      <description>A 16-byte IPv6 address is present</description>
      <xref type="rfc" data="rfc4025"/>
    </record>
    <record>
      <value>3</value>
      <description>A wire-encoded domain name is present</description>
      <xref type="rfc" data="rfc4025"/>
    </record>
    <record>
      <value>4-255</value>
      <description>Unassigned</description>
    </record>
  </registry>
  <people/>
</registry>
