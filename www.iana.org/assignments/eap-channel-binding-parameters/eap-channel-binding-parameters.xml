<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="eap-channel-binding-parameters.xsl"?>
<?xml-model href="eap-channel-binding-parameters.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="eap-channel-binding-parameters">
  <title>Extensible Authentication Protocol (EAP) Channel-Binding Parameters</title>
  <created>2012-06-08</created>
  <updated>2013-10-31</updated>
  
  <registry id="channel-binding-codes">
    <title>EAP Channel-Binding Codes</title>
    <xref type="rfc" data="rfc6677"/>
    <registration_rule>Standards Action</registration_rule>
    <record>
      <value>0</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc6677"/>
    </record>
    <record>
      <value>1</value>
      <description>Channel-binding data from client</description>
      <xref type="rfc" data="rfc6677"/>
    </record>
    <record>
      <value>2</value>
      <description>Channel-binding response: success</description>
      <xref type="rfc" data="rfc6677"/>
    </record>
    <record>
      <value>3</value>
      <description>Channel-binding response: failure</description>
      <xref type="rfc" data="rfc6677"/>
    </record>
    <record>
      <value>4-255</value>
      <description>Unassigned</description>
    </record>
  </registry>
  
  <registry id="channel-binding-namespaces">
    <title>EAP Channel-Binding Namespaces</title>
    <xref type="rfc" data="rfc6677"/>
    <registration_rule>IETF Review</registration_rule>
    <record>
      <value>0</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc6677"/>
    </record>
    <record>
      <value>1</value>
      <description>RADIUS</description>
      <xref type="rfc" data="rfc6677"/>
    </record>
    <record>
      <value>2-254</value>
      <description>Unassigned</description>
    </record>
    <record>
      <value>255</value>
      <description>Reserved for Private Use</description>
      <xref type="rfc" data="rfc6677"/>
    </record>
  </registry>
  
  <people/>
</registry>
