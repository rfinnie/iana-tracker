<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="epp-extension-role-values.xsl"?>
<?oxygen RNGSchema="epp-extension-role-values.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="epp-extension-role-values">
  <title>Extensible Provisioning Protocol (EPP) Organization Role Values</title>
  <created>2018-12-26</created>
  <updated>2019-12-09</updated>
  
  <registry id="epp-extension-role-values-1">
    <title>EPP Organization Role Values</title>
    <xref type="rfc" data="rfc8543"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>James Gould</expert>  
    <record date="2018-12-26">
      <value>registrar</value>
      <description>The entity object instance represents the authority
        responsible for the registration in the registry.</description>
      <registrant><xref type="person" data="IESG"/></registrant>
      <xref type="rfc" data="rfc8543"/>
    </record>
    <record date="2018-12-26">
      <value>reseller</value>
      <description>The entity object instance represents a third party
        through which the registration was conducted (i.e., not the
        registry or registrar).</description>
      <registrant><xref type="person" data="IESG"/></registrant>
      <xref type="rfc" data="rfc8543"/>
    </record>
    <record date="2018-12-26">
      <value>privacyproxy</value>
      <description>The entity object instance represents a third party
        who could help to register a domain without exposing the
        registrants' private information.</description>
      <registrant><xref type="person" data="IESG"/></registrant>
      <xref type="rfc" data="rfc8543"/>
    </record>
    <record date="2018-12-26">
      <value>dns-operator</value>
      <description>The entity object instance represents a third-party
        DNS operator that maintains the name servers and zone data on
        behalf of a registrant.</description>
      <registrant><xref type="person" data="IESG"/></registrant>
      <xref type="rfc" data="rfc8543"/>
    </record>
  </registry>
  
  <people>
    <person id="IESG">
      <name>IESG</name>
      <uri>mailto:iesg&amp;ietf.org</uri>
    </person> 
  </people>
  
</registry>
